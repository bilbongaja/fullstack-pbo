<div class="container">
    <div class="vm">
        <div class="text1">
            <h1><b>Vicion</b></h1>
            <br>
            <p>Accelerate the development <br> of infrastructure and smart <br>digital platforms that are sustainable,<br> economical and accessible to all people.
            </p>
        </div>
        <div class="text2">
            <h1><b>Mision</b></h1>
            <br>
            <p>SM Telkom has a mission that<br> is to provide service excellence to customers,<br> namely excellent service to customers and carry <br>out improvements related to telecommunications developments.
            </p>
        </div>
    </div>
</div>

<center>
    <div class="team">
        <h1>SM Team</h1>
        <div class="bg-table">
            <table>
                <tr>
                    <th>Nama</th>
                    <th>Jabatan</th>
                    <th>Skill</th>
                    <!-- <th colspan="3">Action</th> -->
                </tr>
                <% data.forEach(row=> { %>
                    <tr>
                        <td>
                            <%= row.employeeName %>
                        </td>
                        <td>
                            <%= row.position %>
                        </td>
                        <td>
                            <%= row.skill %>
                        </td>
                    </tr>
                <% }) %>
            </table>
        </div>
    </div>
</center>